{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "esModuleInterop": true,
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts*",
    "test/**/*.ts*",
    "vite.config.ts"
  ]
}