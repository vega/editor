{
  "name": "vega-editor",
  "version": "0.1.0",
  "homepage": "http://vega.github.io/editor",
  "devDependencies": {
    "autoprefixer": "8.0.0",
    "awesome-typescript-loader": "3.4.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.2.2",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.1",
    "chokidar": "^2.0.2",
    "connect-history-api-fallback": "1.5.0",
    "copy-webpack-plugin": "^4.4.1",
    "cross-spawn": "6.0.4",
    "css-loader": "0.28.9",
    "detect-port": "1.2.2",
    "dotenv": "5.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.18.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.44.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "filesize": "3.6.0",
    "find-cache-dir": "1.0.0",
    "fs-extra": "5.0.0",
    "gh-pages": "^1.1.0",
    "gzip-size": "4.1.0",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-middleware": "0.17.4",
    "jest": "22.3.0",
    "json-loader": "0.5.7",
    "object-assign": "4.1.1",
    "path-exists": "3.0.0",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.0",
    "recursive-readdir": "2.2.1",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.3",
    "strip-ansi": "4.0.0",
    "style-loader": "0.20.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "d3-request": "1.0.6",
    "deep-equal": "^1.0.1",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-monaco-editor": "^0.13.0",
    "react-portal": "^4.1.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-split-pane": "^0.1.77",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "vega": "github:vega/vega#v3.0.10",
    "vega-datasets": "^1.16.0",
    "vega-lite": "github:vega/vega-lite#v2.1.3",
    "vega-schema-url-parser": "^1.0.0",
    "vega-tooltip": "^0.5.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "generate-example-images": "./scripts/generate-example-images.sh",
    "test": "node scripts/test.js --env=jsdom",
    "postinstall": "npm run vendor",
    "vendor": "./scripts/vendor.sh",
    "lint": "eslint src",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ],
      "space-before-function-paren": [
        "warn",
        "never"
      ],
      "object-curly-spacing": [
        "warn",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  }
}
