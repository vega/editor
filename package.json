    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "@types/d3": "^7.0.0",
    "@types/react": "^17.0.26",
    "@types/react-redux": "^7.1.18",
    "@types/semver": "^7.3.8",
